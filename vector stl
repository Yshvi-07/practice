#include <iostream>
#include<array>
#include<vector>

using namespace std;


int main()
{
    cout<<"Hello World";
    //excess array in c++
     array<int ,5>arr={4,5,8,10,96};
     
     for(int i=0;i<5;i++)
     {
         cout<<arr.at(i)<<" ";
     }
     cout<<endl;
     //EXCESS AT a particular position
     cout<<arr.at(2)<<endl;
     //to check if the coresponding value is there or not
     cout<<"Array elemnt is empty or not"<<"  "<<arr.empty()<<endl;
     vector<int>v;
     cout<<"Capacity-"<<v.capacity()<<endl;
     v.push_back(7);
     cout<<"Capacity"<<v.capacity()<<endl;
     cout<<"size-"<<v.size()<<endl;
     
     
     
     vector<int> a(4,2);
     cout<<"5th element is in this position-"<<a.at(2)<<endl;
     a.pop_back();
     for(int i:a)
     cout<<i<<" ";
     
     
     
     
     
     //to copy one vector int other one
     vector<int>last(a);
     cout<<"print last"<<endl;
     for(int i:last){
         cout<<i<<" ";
     }
     cout<<endl;
     vector<int> g1;
  
    for (int i = 1; i <= 5; i++)
        g1.push_back(i);
     for (int i = 1; i <= 5; i++)
     cout<<i<<endl;
     
    return 0;
}
